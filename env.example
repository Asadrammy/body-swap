# Face and Body Swap Pipeline Configuration

# Device Configuration
DEVICE=cuda
CUDA_AVAILABLE=true

# Processing Configuration
IMAGE_SIZE=512
MAX_IMAGE_SIZE=1024

# Paths
MODELS_DIR=models
OUTPUTS_DIR=outputs
TEMP_DIR=temp

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Model Paths (optional, will download if not specified)
# INSIGHTFACE_MODEL_PATH=models/insightface
# SAM_MODEL_PATH=models/sam/sam_vit_h_4b8939.pth
# OPENPOSE_MODEL_PATH=models/openpose

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# HuggingFace Token (optional, for private models)
# HF_TOKEN=your_token_here

# Google AI Studio API Key (optional, for enhanced quality assessment)
# GOOGLE_AI_API_KEY=your_google_ai_api_key_here
# GEMINI_API_KEY=your_google_ai_api_key_here  # Alternative name

# AI Image Generation API Keys (for image generation without local models)
# Set USE_AI_API=true to use AI APIs instead of local models (avoids distortion)
USE_AI_API=true
# Google AI Studio API Key (recommended - uses Imagen for image generation)
# GOOGLE_AI_API_KEY=your_google_ai_api_key_here
# GEMINI_API_KEY=your_google_ai_api_key_here  # Alternative name
# OpenAI DALL-E API Key (optional)
# OPENAI_API_KEY=your_openai_api_key_here
# Stability AI API Key (optional, for inpainting)
# STABILITY_API_KEY=your_stability_api_key_here
# Replicate API Token (optional)
# REPLICATE_API_TOKEN=your_replicate_token_here
# AI Provider selection: "google", "openai", "stability", or "replicate" (default: google if available)
# AI_IMAGE_PROVIDER=google

